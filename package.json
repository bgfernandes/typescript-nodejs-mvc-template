{
  "name": "typescript-nodejs-mvc-template",
  "version": "1.0.0",
  "description": "A template for traditional server-side rendered MVC application using NodeJS and TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=development ts-node src/server.ts",
    "start-prod": "npm run build-prod && NODE_ENV=production ts-node src/server.ts",
    "build": "npm run tailwind-css",
    "build-prod": "npm run tailwind-css-prod",
    "lint": "eslint . --ext .ts",
    "dev": "nodemon --watch './src/**/*.*' --exec 'npm run start'",
    "test": "jest --runInBand",
    "tailwind-css": "npx tailwindcss -i ./src/assets/styles/tailwind.css -o ./public/tailwind.css",
    "tailwind-css-prod": "NODE_ENV=production npx tailwindcss -i ./src/assets/styles/tailwind.css -o ./public/tailwind.css",
    "db:setup": "npm run db:create && npm run db:migrate",
    "db:create": "ts-node db/createDatabase.ts",
    "db:drop": "ts-node db/dropDatabase.ts",
    "db:migrate": "knex migrate:latest",
    "db:migrate:rollback": "knex migrate:rollback"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bgfernandes/typescript-nodejs-mvc-template.git"
  },
  "author": "github.com/bgfernandes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bgfernandes/typescript-nodejs-mvc-template/issues"
  },
  "homepage": "https://github.com/bgfernandes/typescript-nodejs-mvc-template#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/i18n": "^0.13.0",
    "@types/jest": "^26.0.23",
    "@types/json-schema": "^7.0.7",
    "@types/nunjucks": "^3.1.4",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "autoprefixer": "^10.2.6",
    "copyfiles": "^2.4.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "fishery": "^1.3.1",
    "jest": "^27.0.5",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.5",
    "supertest": "^6.1.3",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "i18n": "^0.13.3",
    "knex": "^0.95.6",
    "moment": "^2.29.1",
    "nunjucks": "^3.2.3",
    "objection": "^2.2.15",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.6.0",
    "ts-node": "^10.0.0"
  }
}
